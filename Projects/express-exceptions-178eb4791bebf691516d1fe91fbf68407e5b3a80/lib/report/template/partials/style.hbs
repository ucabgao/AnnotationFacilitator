<style type="text/css">
	html, body, div, span, object, iframe,
	h1, h2, h3, h4, h5, h6, p, blockquote, pre,
	abbr, address, cite, code,
	del, dfn, em, img, ins, kbd, q, samp,
	small, strong, sub, sup, var,
	b, i,
	dl, dt, dd, ol, ul, li,
	fieldset, form, label, legend,
	table, caption, tbody, tfoot, thead, tr, th, td,
	article, aside, canvas, details, figcaption, figure, 
	footer, header, hgroup, menu, nav, section, summary,
	time, mark, audio, video {
	    margin:0;
	    padding:0;
	    border:0;
	    outline:0;
	    font-size:100%;
	    vertical-align:baseline;
	    background:transparent;
	    font-family: 'Trebuchet MS', 'Liberation Sans', 'DejaVu Sans', sans-serif;
	}

	body {
	    line-height:1;
	}

	article,aside,details,figcaption,figure,
	footer,header,hgroup,menu,nav,section { 
	    display:block;
	}

	nav ul {
	    list-style:none;
	}

	blockquote, q {
	    quotes:none;
	}

	blockquote:before, blockquote:after,
	q:before, q:after {
	    content:'';
	    content:none;
	}

	a {
	    margin:0;
	    padding:0;
	    font-size:100%;
	    vertical-align:baseline;
	    background:transparent;
	}

	table {
	    border-collapse:collapse;
	    border-spacing:0;
	}

	/* change border colour to suit your needs */
	hr {
	    display:block;
	    height:1px;
	    margin: 0 0 15px;
	    border: 0;
	    background-image: -webkit-linear-gradient(left, rgba(0,0,0,0.2), rgba(0,0,0,0.01));
	    background-image: -moz-linear-gradient(left, rgba(0,0,0,0.2), rgba(0,0,0,0.01));
	    background-image: -ms-linear-gradient(left, rgba(0,0,0,0.2), rgba(0,0,0,0.01));
	    background-image: -o-linear-gradient(left, rgba(0,0,0,0.2), rgba(0,0,0,0.01));
	}

	/*

	Original style from softwaremaniacs.org (c) Ivan Sagalaev <Maniac@SoftwareManiacs.Org>

	*/

	pre code, pre, code, .hljs, .file-table, .file-location {
	    font: normal 14px Ubuntu Mono; 
	}

	.hljs {
	  overflow-x: auto;
	  padding: 0.3em;
	 background: #23241f;
	  -webkit-text-size-adjust: none;
	}

	.hljs,
	.hljs-tag,
	.css .hljs-rules,
	.css .hljs-value,
	.css .hljs-function
	.hljs-preprocessor,
	.hljs-pragma {
	  color: #f8f8f2;
	}

	.hljs-strongemphasis,
	.hljs-strong,
	.hljs-emphasis {
	  color: #a8a8a2;
	}

	.hljs-bullet,
	.hljs-blockquote,
	.hljs-horizontal_rule,
	.hljs-number,
	.hljs-regexp,
	.alias .hljs-keyword,
	.hljs-literal,
	.hljs-hexcolor {
	  color: #ae81ff;
	}

	.hljs-tag .hljs-value,
	.hljs-code,
	.hljs-title,
	.css .hljs-class,
	.hljs-class .hljs-title:last-child {
	  color: #a6e22e;
	}

	.hljs-link_url {
	  font-size: 80%;
	}

	.hljs-strong,
	.hljs-strongemphasis {
	  font-weight: bold;
	}

	.hljs-emphasis,
	.hljs-strongemphasis,
	.hljs-class .hljs-title:last-child {
	  font-style: italic;
	}

	.hljs-keyword,
	.hljs-function,
	.hljs-change,
	.hljs-winutils,
	.hljs-flow,
	.nginx .hljs-title,
	.tex .hljs-special,
	.hljs-header,
	.hljs-attribute,
	.hljs-symbol,
	.hljs-symbol .hljs-string,
	.hljs-tag .hljs-title,
	.hljs-value,
	.alias .hljs-keyword:first-child,
	.css .hljs-tag,
	.css .unit,
	.css .hljs-important {
	  color: #f92672;
	}

	.hljs-function .hljs-keyword,
	.hljs-class .hljs-keyword:first-child,
	.hljs-constant,
	.css .hljs-attribute {
	  color: #66d9ef;
	}

	.hljs-variable,
	.hljs-params,
	.hljs-class .hljs-title {
	  color: #f8f8f2;
	}

	.hljs-string,
	.css .hljs-id,
	.hljs-subst,
	.hljs-type,
	.ruby .hljs-class .hljs-parent,
	.hljs-built_in,
	.django .hljs-template_tag,
	.django .hljs-variable,
	.smalltalk .hljs-class,
	.django .hljs-filter .hljs-argument,
	.smalltalk .hljs-localvars,
	.smalltalk .hljs-array,
	.hljs-attr_selector,
	.hljs-pseudo,
	.hljs-addition,
	.hljs-stream,
	.hljs-envvar,
	.apache .hljs-tag,
	.apache .hljs-cbracket,
	.tex .hljs-command,
	.hljs-prompt,
	.hljs-link_label,
	.hljs-link_url {
	  color: #e6db74;
	}

	.hljs-comment,
	.hljs-javadoc,
	.hljs-annotation,
	.hljs-decorator,
	.hljs-template_comment,
	.hljs-pi,
	.hljs-doctype,
	.hljs-deletion,
	.hljs-shebang,
	.apache .hljs-sqbracket,
	.tex .hljs-formula {
	  color: #75715e;
	}

	.coffeescript .javascript,
	.javascript .xml,
	.tex .hljs-formula,
	.xml .javascript,
	.xml .vbscript,
	.xml .css,
	.xml .hljs-cdata,
	.xml .php,
	.php .xml {
	  opacity: 0.5;
	}


	h1, h2, h3, h4 {
	    line-height: 1.4;
	    margin: 5px 0 10px;
	}

	h1 {
	    font-size: 28px;
	}

	h2 {
	    font-size: 20px;
	}

	h3 {
	    font-size: 18px;
	}

	h4 {
	    font-size: 16px;
	}

	p {
	    line-height: 1.2;
	    margin: 5px 0;
	}

	.caret {
	    display: inline-block;
	    width: 0;
	    height: 0;
	    margin-left: 2px;
	    vertical-align: middle;
	    border-top: 6px solid;
	    border-right: 6px solid transparent;
	    border-left: 6px solid transparent;
	}

	.active .caret {
	    margin-top: -6px;
	    border-top: 6px solid transparent;
	    border-bottom: 6px solid;
	    border-right: 6px solid transparent;
	    border-left: 6px solid transparent;   
	}

	#page {
	    width: auto;
	    margin: 20px;
	    height: 100%;
	    box-sizing: border-box;
	}

	.headline {
	    margin: 5px 0;
	    overflow: hidden;
	}

	.headline > h2, .headline > p {
	    line-height: 30px;
	    display: inline-block;
	}

	.headline > h2 {
	    color: #333;
	    font-size: 26px;
	}

	.headline.error > h2 {
	    color: #FF0000;
	}

	.headline > p {
	    font-size: 20px;
	}

	.error-message {
	    background: #EEE;
	    padding: 5px 10px;
	    color: #333;
	    margin: 0 0 10px 0;
	}

	.section {
	    margin: 10px 0;
	    padding: 10px;
	    background: #FAFAFA;
	    box-shadow: 0 5px 5px #EEE;
	}

	.cause-wrapper {
	    margin-left: 25px;
	}

	.cause-headline {
	    margin: 5px 0;
	    line-height: 30px;
	}

	.warning {
	    color: #FF0000 !important;
	}

	.file-location {
	    margin-bottom: 10px;
	}

	.dropdown-trigger {
	    color: #333;
	    display: block;
	    text-decoration: none;
	    overflow: hidden;
	}

	.dropdown-trigger:hover {
	    color: #f92672;
	}

	.file-dropdown {
	    display: none;
	    overflow: hidden;
	    -moz-border-radius: 5px;
	    -webkit-border-radius: 5px;
	    -khtml-border-radius: 5px;
	    border-radius: 5px;
	    box-shadow: inset 0 0px 5px #000;
	}

	.stack-list li {
	    margin-left: 10px;
	    border-bottom: 1px solid #EEE;
	}

	.stack-list > li:first-child > .file-dropdown {
	    display: block;
	}

	.file-table {
	    border-collapse: collapse;
	    width: 100%;
	}

	tr.error > td:last-child {
	    background: #191919;
	    box-shadow: inset 0 0px 3px #000;
	}

	.file-table td {
	    padding: 0.3em;
	    background: #23241f;
	    vertical-align: middle;
	}

	.file-table td.file-line-number {
	    text-align: right;
	    width: 40px;
	    background: #202020;
	    border-right: 1px solid #000;
	}

	.file-line-number > span {
	    padding-right: 5px;
	    color: #ACACAC !important;
	}

	p.property-key, p.property-value {
	    display: inline-block;
	    vertical-align: middle;
	}

	p.property-key:after {
	    content: ':'
	}

	p.property-value {
	    font: normal 14px Ubuntu Mono; 
	    color: #0000FF;
	}

	ul, ul > li {
	    list-style: none;
	}

	ul > li.environment-list-item {
	    margin: 0 20px;  
	}

</style>